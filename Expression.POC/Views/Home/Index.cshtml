  <div class="align-content-center">

    <input type="text" name="txtFormula" id="txtFormula" />
    <p id="lblExpressao" hidden="hidden"></p>
    <button type="button" id="btnTestar">Testar</button>

  </div>
  <script>
    $(function () {

      $("#btnTestar").click(function () {
        console.log('entrou');
        $.ajax({
          method: "POST",
          url: "Home/Testar",
          data: { expr: $('#lblExpressao').html() }
        })
      });


      $('#txtFormula').keyup(function (event) {

        if (!this.value) {
          $('#lblExpressao').html("");
        }
      });

      $("#txtFormula").keypress(function (event) {
        var regex = /[x(+)/-]/g;
        var text;

        const number = 1;
        var expr = $('#lblExpressao').html();

        event = event || window.event;
        var charCode = (typeof event.which == "number") ? event.which : event.keyCode;
        if (charCode > 0) {
          text = String.fromCharCode(charCode);
        }

        if (text != null) {
          var found = text.match(regex);
          if (found != null) {
            var regex2 = /[+-x/(]/g;

            if (text.match(regex2)) {
              expr += text + number;
            } else {
              expr += text;
            }
            $('#lblExpressao').html(expr);
          }
        }
        else {
          alert('Texto n√£o identificado');
        }
      });
    });
  </script>

